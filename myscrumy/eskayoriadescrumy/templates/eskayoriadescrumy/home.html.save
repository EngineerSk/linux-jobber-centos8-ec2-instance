<style>
    table, td, tr{
      border:1px solid red;
      border-collapse:collapse;
    }
</style>

<table> 
   <thead>
      <tr>
        <td>
          User
        </td>
        <td>
          Weekly Goals
        </td>
        <td>
          Daily Goals
        </td>
        <td>
          Verify Goals
        </td>
        <td>
          Done Goals
        </td>
      </tr>
   </thead>
   <tbody>
         {% for goal in goals %}
           {% for status in goal_status %}
             
              <tr>
                 <td> {{ goal.user.first_name }} {{ goal.goal_status }}</td>
                 <td> {{ goal.goal_name }}{{ goal.goal_id }} </td>
                 <td></td>
                 <td></td>
              </tr>
           {% endfor %}
         {% endfor %}  
      </tr>
   </tbody>
</table>
